{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "esbuild": "^0.14.48",
    "esbuild-sass-plugin": "^2.2.6",
    "eslint": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "esbuild react-src/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "typecheck": "tsc --project tsconfig.json --noEmit --preserveWatchOutput",
    "typecheck:watch": "tsc --project tsconfig.json --noEmit --watch --preserveWatchOutput",
    "lint": "eslint --quiet --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern ./react-src/**/*",
    "lint:fix": "eslint --fix --quiet --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern ./react-src/**/*",
    "brakeman": "brakeman -o log/brakeman.out",
    "stylelint": "stylelint react-src/**/*.s[ac]ss",
    "stylelint:fix": "stylelint --fix react-src/**/*.s[ac]ss",
    "test": "jest --silent",
    "test:all": "yarn typecheck && yarn lint && yarn brakeman && bundle exec rspec && yarn test"
  }
}
